{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React webpack-based boilerplate to create some awesome react apps",
  "repository": "https://github.com/bkhanale/react-boilerplate",
  "author": "Bhushan Khanale",
  "license": "MIT",
  "scripts": {
